    <fields>
        <fullName>zeitzone__MailingLTFactor__c</fullName>
        <deprecated>false</deprecated>
        <description>Local Time Factor: TimeZone Factor / 24</description>
        <externalId>false</externalId>
        <label>MailingLTFactor</label>
        <precision>18</precision>
        <required>false</required>
        <scale>16</scale>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>zeitzone__MailingLocalTime__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>if( IsBlank(zeitzone__MailingTimeZoneId__c) , &apos;&apos;, substitute(mid( TEXT (Now()+ zeitzone__MailingLTFactor__c),6,5),&apos;-&apos;,&apos;/&apos;) &amp; &apos;/&apos; &amp; left( TEXT (Now()+ zeitzone__MailingLTFactor__c),4) &amp; &apos; &apos; 
&amp; 
if( value(mid( TEXT (Now() + zeitzone__MailingLTFactor__c),12,2)) &gt; 12 
, text(mod(value(mid( TEXT (Now() + zeitzone__MailingLTFactor__c),12,2)),12)) &amp; mid( TEXT (Now() + zeitzone__MailingLTFactor__c),14,3) &amp; &apos; PM&apos; 
, mid( TEXT (Now()+ zeitzone__MailingLTFactor__c),12,5) &amp; case( value(mid( TEXT (Now()+ zeitzone__MailingLTFactor__c),12,2))/12 , 1, &apos; PM&apos;, &apos; AM&apos;) ) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Mailing Local Time</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>zeitzone__MailingTimeZoneId__c</fullName>
