        <formula>if( IsBlank(zeitzone__otherTimeZoneId__c) , &apos;&apos;, substitute(mid( TEXT (Now()+ zeitzone__OtherLTFactor__c),6,5),&apos;-&apos;,&apos;/&apos;) &amp; &apos;/&apos; &amp; left( TEXT (Now()+ zeitzone__OtherLTFactor__c),4) &amp; &apos; &apos; 
