    <fields>
        <fullName>zeitzone__BillingLocalTime__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>if( isBlank(zeitzone__BillingTimeZoneId__c), &apos;&apos;, substitute(mid( TEXT (Now()+ zeitzone__BillingLTFactor__c),6,5),&apos;-&apos;,&apos;/&apos;) &amp; &apos;/&apos; &amp; left( TEXT (Now()+ zeitzone__BillingLTFactor__c),4) &amp; &apos; &apos; 
&amp; 
if( value(mid( TEXT (Now() + zeitzone__BillingLTFactor__c),12,2)) &gt; 12 
, text(mod(value(mid( TEXT (Now() + zeitzone__BillingLTFactor__c),12,2)),12)) &amp; mid( TEXT (Now() + zeitzone__BillingLTFactor__c),14,3) &amp; &apos; PM&apos; 
, mid( TEXT (Now()+ zeitzone__BillingLTFactor__c),12,5) &amp; case( value(mid( TEXT (Now()+ zeitzone__BillingLTFactor__c),12,2))/12 , 1, &apos; PM&apos;, &apos; AM&apos;) ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Billing Local Time</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
