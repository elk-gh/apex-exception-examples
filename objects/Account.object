        <formula>if( IsBlank(zeitzone__ShippingTimeZoneId__c), &apos;&apos;,  substitute(mid( TEXT (Now()+ zeitzone__ShippingLTFactor__c),6,5),&apos;-&apos;,&apos;/&apos;) &amp; &apos;/&apos; &amp; left( TEXT (Now()+ zeitzone__ShippingLTFactor__c),4) &amp; &apos; &apos; 
&amp; 
if( value(mid( TEXT (Now() + zeitzone__ShippingLTFactor__c),12,2)) &gt; 12 
